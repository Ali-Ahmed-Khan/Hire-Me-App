doctype html
html(lang='en')
  head
    title Hire Me - The Hacking School Bootcamper
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    // ===============================================================================================
    link(rel='icon', type='image/png', href='https://colorlib.com/etc/cf/ContactFrom_v5/images/icons/favicon.ico')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/bootstrap/css/bootstrap.min.css')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/fonts/font-awesome-4.7.0/css/font-awesome.min.css')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/fonts/iconic/css/material-design-iconic-font.min.css')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/animate/animate.css')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/css-hamburgers/hamburgers.min.css')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/animsition/css/animsition.min.css')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/select2/select2.min.css')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/daterangepicker/daterangepicker.css')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/noui/nouislider.min.css')
    // ===============================================================================================
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/css/util.css')
    link(rel='stylesheet', type='text/css', href='https://colorlib.com/etc/cf/ContactFrom_v5/css/main.css')
    // ===============================================================================================
  body
    .container-contact100
      .wrap-contact100
        form.contact100-form.validate-form
          span.contact100-form-title
            h3 Hire Me - The Hacking School Bootcamper
          .wrap-input100.validate-input.bg1(data-validate='Please Type Your Name')
            span.label-input100 FULL NAME *
            input.input100(type='text', name='name', placeholder='Enter Your Name')
          .wrap-input100.validate-input.bg1(data-validate='Enter Your Email (e@a.x)')
            span.label-input100 Email *
            input.input100(type='text', name='email', placeholder='Enter Only Valid Email ')
          .wrap-input100.input100-select.bg1
            span.label-input100 Needed Services *
            div
              select.js-select2(name='service')
                option My available Services
                option Build Personal Website
                option UI/UX Design
                option Backend Development
                option React Development
              .dropDownSelect2
          .w-full.dis-none.js-show-service
            .wrap-contact100-form-range
              span.label-input100 Budget *
              .contact100-form-range-value
                | $
                span#value-lower 610
                |  - $
                span#value-upper 980
                input(type='text', name='from-value')
                input(type='text', name='to-value')
              .contact100-form-range-bar
                #filter-bar
          .wrap-input100.validate-input.bg0.rs1-alert-validate(data-validate='Please Type Your Message')
            span.label-input100 Message
            textarea.input100(name='message', placeholder='Enter job description')
          .container-contact100-form-btn
            button.contact100-form-btn
              span
                | Submit
                i.fa.fa-long-arrow-right.m-l-7(aria-hidden='true')
    // ===============================================================================================
    script(src='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/jquery/jquery-3.2.1.min.js')
    // ===============================================================================================
    script(src='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/animsition/js/animsition.min.js')
    // ===============================================================================================
    script(src='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/bootstrap/js/popper.js')
    script(src='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/bootstrap/js/bootstrap.min.js')
    // ===============================================================================================
    script(src='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/select2/select2.min.js')
    script.
      $(".js-select2").each(function () {
      $(this).select2({
      minimumResultsForSearch: 20,
      dropdownParent: $(this).next('.dropDownSelect2')
      });
      $(".js-select2").each(function () {
      $(this).on('select2:close', function (e) {
      if ($(this).val() == "Please chooses") {
      $('.js-show-service').slideUp();
      } else {
      $('.js-show-service').slideUp();
      $('.js-show-service').slideDown();
      }
      });
      });
      })
    // ===============================================================================================
    script(src='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/daterangepicker/moment.min.js')
    script(src='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/daterangepicker/daterangepicker.js')
    // ===============================================================================================
    script(src='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/countdowntime/countdowntime.js')
    // ===============================================================================================
    script(src='https://colorlib.com/etc/cf/ContactFrom_v5/vendor/noui/nouislider.min.js')
    script.
      var filterBar = document.getElementById('filter-bar');
      noUiSlider.create(filterBar, {
      start: [1500, 3900],
      connect: true,
      range: {
      'min': 1500,
      'max': 7500
      }
      });
      var skipValues = [
      document.getElementById('value-lower'),
      document.getElementById('value-upper')
      ];
      filterBar.noUiSlider.on('update', function (values, handle) {
      skipValues[handle].innerHTML = Math.round(values[handle]);
      $('.contact100-form-range-value input[name="from-value"]').val($('#value-lower').html());
      $('.contact100-form-range-value input[name="to-value"]').val($('#value-upper').html());
      });
    // ===============================================================================================
    script(src='js/main.js')
